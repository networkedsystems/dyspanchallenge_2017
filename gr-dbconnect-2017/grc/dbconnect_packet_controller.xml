<?xml version="1.0"?>
<block>
  <name>Packet controller</name>
  <key>dbconnect_packet_controller</key>
  <category>[dbconnect]</category>
  <import>import dbconnect</import>
  <make>dbconnect.packet_controller($samp_rate, $swtime, $delay_1, $delay_2, $tconst, $mean1, $mean2, $mean3, $seed, $gain_period, $gain_vals, $scen_list, $rand_scen)</make>
	<param>
		<name>Sample rate</name>
		<key>samp_rate</key>
		<value>10e6</value>
		<type>real</type>
	</param>
    <param>
      <name>Swtime</name>
      <key>swtime</key>
      <value>60000</value>
      <type>int</type>
    </param>
    <param>
      <name>Delay-1</name>
      <key>delay_1</key>
      <value>5</value>
      <type>int</type>
    </param>
    <param>
      <name>Delay-2</name>
      <key>delay_2</key>
      <value>100</value>
      <type>int</type>
    </param>
    <param>
      <name>Tconst</name>
      <key>tconst</key>
      <value>0</value>
      <type>int</type>
    </param>
    <param>
      <name>Mean1</name>
      <key>mean1</key>
      <value>10</value>
      <type>int</type>
    </param>
    <param>
      <name>Mean2</name>
      <key>mean2</key>
      <value>100</value>
      <type>int</type>
    </param>
    <param>
      <name>Mean3</name>
      <key>mean3</key>
      <value>50</value>
      <type>int</type>
    </param>
    <param>
      <name>Seed</name>
      <key>seed</key>
      <value>6643</value>
      <type>int</type>
    </param>
	<param>
		<name>Gain values</name>
		<key>gain_vals</key>
		<type>raw</type>
	</param>
	<param>
		<name>Scenario list</name>
		<key>scen_list</key>
		<type>raw</type>
	</param>
	<param>
      <name>Gain_per</name>
      <key>gain_period</key>
      <value>60000</value>
      <type>int</type>
    </param>
	<param>
		<name>Randomize Scenarios</name>
		<key>rand_scen</key>
		<value>True</value>
		<type>bool</type>
		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>
  <sink>
   <name>in0</name>
   <type>message</type>
  </sink>
  <source>
    <name>cmd</name>
    <type>message</type>
  </source>
  <source>
    <name>gcmd</name>
    <type>message</type>
  </source>
  <source>
    <name>out0</name>
    <type>complex</type>
  </source>
  <source>
    <name>out1</name>
    <type>complex</type>
  </source>
  <source>
    <name>out2</name>
    <type>complex</type>
  </source>
  <source>
    <name>out3</name>
    <type>complex</type>
  </source>
</block>
